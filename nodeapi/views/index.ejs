<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <p><%= valor %></p>

    <% if (condicion.estado) { %>
        <p><%= condicion.segundo %> es par </p>
    <% } else { %>
        <p><%= condicion.segundo %> es impar </p>
    <% } %>

    <% users.forEach(function(user){ %>
        <p><%= user.name + ' ' + user.age %><p>
    <% }) %>
  </body>
</html>
